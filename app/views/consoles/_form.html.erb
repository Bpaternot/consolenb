<%= simple_form_for @console do |f| %>
  <%= f.input :brand, collection: Console::BRAND  %>
  <%= f.input :shifter, collection: 1..8 %>
  <%= f.input :description, as: :text, label: "description" %>
  <%= f.input :address, as: :string %>
  <%= f.input :price_per_day, as: :integer %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>

